package javaseapp.thread;

import javax.swing.*;

public class ProgressApp extends JFrame {
	JProgressBar bar;
	JButton bt;
	
	public ProgressApp() {
		bar = new JProgressBar();
		bt = new JButton("download");
		
		setSize(400,300);
		setVisible(true);
		
	}
	
	public static void main(String[] args) {
		new ProgressApp();
	}
}
